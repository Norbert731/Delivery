@model Delivery.Models.AutomatPaczkowy

@{
    ViewData["Title"] = "Create Automat Paczkowy";
}

<h1>@ViewData["Title"]</h1>

<form asp-action="Create" method="post">
    <div class="form-group">
        <label asp-for="Lokalizacja" class="control-label"></label>
        <input asp-for="Lokalizacja" class="form-control" />
        <span asp-validation-for="Lokalizacja" class="text-danger"></span>
    </div>

    <button type="submit" class="btn btn-primary">Create</button>
    <a href="@Url.Action("Index", "AutomatPaczkowy")" class="btn btn-secondary">Back to List</a>
</form>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
