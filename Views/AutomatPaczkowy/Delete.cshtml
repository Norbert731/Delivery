@model Delivery.Models.AutomatPaczkowy

@{
    ViewData["Title"] = "Delete Automat Paczkowy";
}

<h1>@ViewData["Title"]</h1>

<h4>Are you sure you want to delete this?</h4>
<hr />
<dl class="row">
    <dt class="col-sm-2">ID</dt>
    <dd class="col-sm-10">@Model.ID_automat</dd>

    <dt class="col-sm-2">Lokalizacja</dt>
    <dd class="col-sm-10">@Model.Lokalizacja</dd>
</dl>

<form method="post" asp-action="Delete">
    <input type="hidden" asp-for="ID_automat" />
    <button type="submit" class="btn btn-danger">Delete</button>
    <a href="@Url.Action("Index", "AutomatPaczkowy")" class="btn btn-secondary">Cancel</a>
</form>
